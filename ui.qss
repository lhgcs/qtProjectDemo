QLabel#labelCard {
    font-size: 28px;
    color: rgb(36,164,206);
    background: transparent;
}

QLabel#label_white {
    background-color:rgb(255,255,255);
}

/**********按钮**********/
QToolButton{
    font-size: 22px;
    border: none;
    color: rgb(29,133,165);
    background: transparent;
    qproperty-toolButtonStyle: ToolButtonTextUnderIcon;
}
QToolButton:hover{
    color: rgb(255,233,0 );
}
QToolButton#btnMoney{
    qproperty-icon: url("/home/ico/pay1.png");
    qproperty-iconSize: 200px 200px;
    qproperty-toolButtonStyle: ToolButtonTextBesideIcon;
}
QToolButton#btnMobile{
    qproperty-icon: url("/home/ico/pay2.png");
    qproperty-iconSize: 200px 200px;
    qproperty-toolButtonStyle: ToolButtonTextBesideIcon;
}
QToolButton#btnCard{
    qproperty-icon: url("/home/ico/pay3.png");
    qproperty-iconSize: 200px 200px;
    qproperty-toolButtonStyle: ToolButtonTextBesideIcon;
}

/**********按钮**********/
QPushButton {
    font-size: 24px;
    border: none;
    color: rgb(29,133,165);
    background: transparent;
}
QPushButton:hover {
    color: rgb(36,164,206);
}
QPushButton:pressed {
    padding-left:3px;
    padding-top:3px;
}


QPushButton#btnNormal {
    font-size: 24px;
    border:2px solid rgb(29,133,165);
    color: rgb(29,133,165);
    background: transparent;
}
QPushButton#btnNormal:pressed {
    padding-left:3px;
    padding-top:3px;
    color: rgb(255,255,255);
    background:rgb(29,133,165);
}


QPushButton#btnBack {
    font-size: 24px;
    border: none;
    color: rgb(255,255,255);
    background:rgb(216,18,17);
}
QPushButton#btnBack:hover {
    background:rgb(216,18,17);
}
QPushButton#btnBack:pressed {
    padding-left:3px;
    padding-top:3px;
}


QWidget[objectName=GoodsPresent] {
    background: rgba(255,255,255,220);
}
QWidget[objectName=GoodsPresent] QLabel{
    font-size: 22px;
    color: rgb(29,133,165);
    background: transparent;
}
QWidget[objectName=GoodsPresent] QToolButton {
    font-size: 22px;
    border: none;
    color: rgb(29,133,165);
    background: transparent;
    qproperty-toolButtonStyle: ToolButtonTextUnderIcon;
}
QWidget[objectName=GoodsPresent] QToolButton:pressed{
    color: rgb(255,255,255);
    background: rgb(216,18,17);
}


QWidget[objectName=Login] {
    border-image:url("/home/ico/wine");
}

QWidget[objectName=Replenishment] {
    border-image:url("/home/ico/wine");
}

QWidget[objectName=DialogInstance] {
    border-image:url(/home/ico/dialog.png);
}



