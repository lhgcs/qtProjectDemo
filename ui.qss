QPushButton#btnClose {
    width:60px;
    height:60px;
    border-image:url("/home/ico/close.png");
    icon-size:: 60px 60px;
}
QPushButton#btnClose:hover,#btnClose:pressed{
    border-image:url("/home/ico/close1.png");
}

QPushButton#btnClose2 {
    width:60px;
    height:60px;
    border-image:url("/home/ico/close_white.png");
    icon-size:: 64px 64px;
}
QPushButton#btnClose2:hover,#btnClose2:pressed{
    border-image:url("/home/ico/close_red.png");
}

QPushButton#btnLeftPage {
    border-image:url("/home/ico/top_white.png");
    icon-size:: 64px 64px;
}
QPushButton#btnLeftPage:pressed {
    border-image:url("/home/ico/top_red.png");
}
QPushButton#btnRightPage {
    border-image:url("/home/ico/bottom_white.png");
    icon-size:: 64px 64px;
}
QPushButton#btnRightPage:pressed {
    border-image:url("/home/ico/bottom_red.png");
}

QPushButton#btnSetting{
    qproperty-icon: url("/home/ico/go_setting.png");
    qproperty-iconSize: 60px 60px;
}

QPushButton#btnOk {
    height: 50px;
    color: rgb(255,255,255);
    background: rgb(230,102,102);
}
QPushButton#btnCancel {
    height: 50px;
    color: rgb(255,255,255);
    background: rgb(88,186,206);
}
QPushButton#btnCancel:enabled:hover,#btnOk:enabled:hover{
    border: 1px solid rgb(255,0,54);
}
QPushButton#BtnPay {
    font-size: 26px;
    color: red;
    border-radius:5px;
    border:2px solid red;
}
QPushButton#BtnPay:pressed {
    color: white;
    background: red;
}
QPushButton#BtnPay:!enabled {
    color: black;
    border:3px solid rgb(173,173,173);
    background: rgb(105,105,105);
}

QLineEdit {
    font-size: 32px;
    border: 1px solid rgb(36,164,206);
    color: rgb(36,164,206);
    background: rgb(255,255,255);
}
QLineEdit#loginQLineEdit {
    font-size: 26px;
    border: 0px solid rgb(255,255,255);
    color: rgb(255,255,255);
    background: rgba(0,0,0,50);
}

QStackedWidget {
   background-color:rgba(0,0,0,0);
}

QListView {
   font-size: 36px;
   color: rgb(45,61,87);
   background:rgb(75,108,139);
   show-decoration-selected: 1;
}
QListView::item {
   min-height:80px;
   border-bottom: 1px solid rgb(255,255,255);
}
QListView::item:selected {
   border: none;
   background:rgb(86,181,177);
   color: rgb(255,255,255);
}
QListView::item:hover {
   background: rgb(170,219,216);
   color: rgb(255,255,255);
}

QTableView{
    font-size: 30px;
    background-color: rgb(250, 250, 250);
    alternate-background-color: rgb(172,202,206);
    gridline-color:rgb(255,255,255);
}
QTableView::item:selected {
    background: rgb(99,199,209);
}

QComboBox QAbstractItemView::item {
    font-size: 24px;
    height: 50px;
}


/*RadioButton样式设置*/
QRadioButton::indicator {
    width: 60px;
    height:60px;
}
/*单选框未选中样式*/
QRadioButton::indicator::unchecked {
    border-image: url(/home/ico/radio_white_128.png);
}
QRadioButton::indicator::checked {
    border-image: url(/home/ico/radio_blue_128.png);
}
QRadioButton{
    spacing: 10px;
    font-size: 24px;
}

QCheckBox {
    spacing: 0px;
    font-size: 24px;
}
QCheckBox::indicator {
    width: 60px;
    height:60px;
    spacing: 0px;
}
QCheckBox::indicator::unchecked {
    border-image: url(/home/ico/check_gray.png);
}
QCheckBox::indicator::checked {
    border-image: url(/home/ico/check_blue.png);
}
